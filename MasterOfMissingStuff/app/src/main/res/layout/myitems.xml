<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/button9"
        android:layout_width="62dp"
        android:layout_height="60dp"
        android:background="#A5A4A4"
        android:fontFamily="sans-serif-light"
        android:rotation="-180"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="14sp"
        app:backgroundTint="#A5A4A4"
        app:cornerRadius="30dp"
        app:icon="@drawable/rightarrow"
        app:iconPadding="2"
        app:iconTint="#FFFFFF"
        app:iconTintMode="multiply"
        app:rippleColor="#FFFFFF"
        tools:layout_editor_absoluteX="174dp"
        tools:layout_editor_absoluteY="588dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="219dp"
        android:layout_height="0dp"
        android:layout_marginTop="71dp"
        android:layout_marginBottom="202dp"
        android:contentDescription="@string/moms_logo"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/momslogo" />

    <Button
        android:id="@+id/button3"
        android:layout_width="242dp"
        android:layout_height="0dp"
        android:layout_marginTop="186dp"
        android:layout_marginBottom="67dp"
        android:background="#8A6262"
        android:fontFamily="sans-serif-light"
        android:text="Search for item..."
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="14sp"
        app:backgroundTint="#DFDEDE"
        app:cornerRadius="30dp"
        app:icon="@drawable/magnifier"
        app:iconPadding="0dp"
        app:iconTint="#FFFFFF"
        app:iconTintMode="multiply"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="28dp"
        android:fontFamily="sans-serif-light"
        android:text="My Items"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="156dp"
        tools:layout_editor_absoluteY="288dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="146dp"
        android:layout_marginBottom="78dp"
        android:fontFamily="sans-serif-light"
        android:text="Item 2"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="198dp"
        tools:layout_editor_absoluteY="477dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="146dp"
        android:layout_marginBottom="187dp"
        android:fontFamily="sans-serif-light"
        android:text="Item 1"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="198dp"
        tools:layout_editor_absoluteY="367dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="103dp"
        android:layout_height="102dp"
        app:srcCompat="@drawable/placeholder"
        tools:layout_editor_absoluteX="62dp"
        tools:layout_editor_absoluteY="452dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="103dp"
        android:layout_height="102dp"
        app:srcCompat="@drawable/placeholder"
        tools:layout_editor_absoluteX="62dp"
        tools:layout_editor_absoluteY="332dp" />

</androidx.constraintlayout.widget.ConstraintLayout>